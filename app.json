{
  "expo": {
    "name": "DeskEyes",
    "slug": "desk-eyes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "NSPhotoLibraryUsageDescription": "Приложению нужен доступ к фото для будущих функций"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lekovvv.deskeyes",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#007AFF",
          "sounds": []
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Приложению нужен доступ к фото для будущих функций",
          "savePhotosPermission": "Приложению нужен доступ для сохранения фото",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "caabf50e-d4af-4777-bcd3-91368490f67e"
      }
    }
  }
}
